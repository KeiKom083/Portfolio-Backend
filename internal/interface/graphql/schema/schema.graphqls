type User {
  id: ID!
  name: String!
  email: String!
  createdAt: DateTime!
  updatedAt: DateTime!
}

input SignUpInput {
  name: String!
  email: String!
  password: String!
}

input LoginInput {
  email: String!
  password: String!
}

type AuthPayload {
  user: User!
}

type Query {
  me: User!
}

type Mutation {
  signUp(input: SignUpInput!): User!
  login(input: LoginInput!): AuthPayload!
  logout: Boolean!
}

scalar DateTime
